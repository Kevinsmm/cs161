<script>
  var insertCloseButtonNextToSubmitButton = function($form) {
    $form.find('.hs-submit .actions').addClass('btnContainer')
    var $closeButton = $('.js-closeRequestDemoModal');
    if (!$closeButton.length) { return; }

    var $submitButton = $form.find(':submit');
    var closeButtonClone = $closeButton[0].cloneNode(true);
    $(closeButtonClone).insertAfter($submitButton);
    $closeButton.hide();
  }
  var displayCloseButtonWhenFormSuccessfullySubmits = function(mutationsList) {
    mutationsList.forEach(function(mutation) {
      if ($(mutation.addedNodes[0]).hasClass('submitted-message')) {
        $('.js-closeRequestDemoModal').show();
      }
    })
  }
  // Since hubspot doesn't provide us with a success callback, we'll need to observe the
  // changes to the dom (i.e. the form is removed and the submitted message is added) to know
  // when to display the close button.
  var observeWhenFormContentChanges = function($form) {
    var observer = new MutationObserver(displayCloseButtonWhenFormSuccessfullySubmits);
    observer.observe($form.parent()[0], { childList: true });
  }
  hbspt.forms.create({
    portalId: "3329496",
    formId: "d4cdc2b0-12ce-441c-a7ea-e26e1fe572ec",
