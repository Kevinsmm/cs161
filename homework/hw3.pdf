    submitButtonClass: "btnv7 btnv7-primary",
    locale: 'en',
    translations: {
      en: {
        required: "Please complete this field.",
      }
    },
    onFormReady: function($form) {
      observeWhenFormContentChanges($form);
      insertCloseButtonNextToSubmitButton($form);
    },
  });
</script>
